{
    "name": "brilliance/plugin-key-generator",
    "description": "A utility for Craft CMS Plugin Developers to generate keys for their customers.  Simply a front-end for the API (https://docs.api.craftcms.com/plugin-licenses.html#create-a-plugin-license)",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "plugin key generator"
    ],
    "support": {
        "docs": "https://github.com/brilliancenw/plugin-key-generator/blob/master/README.md",
        "issues": "https://github.com/brilliancenw/plugin-key-generator/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Brilliance",
            "homepage": "https://www.brilliancenw.com/"
        }
    ],
    "require": {
        "craftcms/cms": ">=4.0",
        "guzzlehttp/guzzle": "^7"
    },
    "autoload": {
        "psr-4": {
          "brilliance\\pluginkeygenerator\\": "src/"
        }
    },
    "extra": {
        "name": "Plugin Key Generator",
        "handle": "plugin-key-generator",
        "developer": "Brilliance",
        "developerUrl": "https://www.brilliancenw.com/",
        "documentationUrl": "https://github.com/brilliancenw/plugin-key-generator/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/brilliancenw/plugin-key-generator/master/CHANGELOG.md",
        "components": {
            "pluginKeyGeneratorService": "brilliance\\pluginkeygenerator\\services\\PluginKeyGeneratorService"
        },
        "class": "brilliance\\pluginkeygenerator\\PluginKeyGenerator"
    }
}
